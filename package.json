{
  "name": "mokosh-dst",
  "version": "0.1.0",
  "description": "Deterministic Simulation Testing Tool for Web Applications",
  "keywords": [
    "deterministic",
    "simulation",
    "testing",
    "web"
  ],
  "license": "MIT",
  "author": "GrungeDev",
  "type": "module",
  "main": "index.js",
  "scripts": {
    "build": "tsup --dts --format cjs,esm src/index.ts",
    "build:ci": "tsc --noEmit",
    "clean": "del-cli dist coverage",
    "dev": "tsx watch src/index.ts",
    "format": "prettier --write .",
    "format:pkg": "sort-package-json",
    "lint": "eslint .",
    "lint:fast": "oxlint -c .oxlintrc.json",
    "lint:fix": "eslint . --fix",
    "test": "vitest --coverage",
    "test:ci": "vitest run"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@vitest/coverage-v8": "2.1.4",
    "@vitest/eslint-plugin": "^1.1.8",
    "del-cli": "^6.0.0",
    "eslint": "^9.14.0",
    "globals": "^15.12.0",
    "oxlint": "^0.11.1",
    "prettier": "^3.3.3",
    "sort-package-json": "^2.10.1",
    "tsup": "^8.3.5",
    "tsx": "^4.19.2",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.13.0",
    "vitest": "^2.1.4"
  }
}
